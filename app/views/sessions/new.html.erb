<h1>Log in</h1>
<div class="log_in_page">
  <%= form_with(scope: :session, url: sessions_path, local: true) do |f| %>
    <%= f.label :email %>
    <%= f.email_field :email %>
    <br>
    <div class="log_in_page2">
    <%= f.label :password %>
    <%= f.password_field :password %>
    <%= f.submit "Log in" %>
    </div>
  <% end %>
</div>
